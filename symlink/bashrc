#!/bin/bash

if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi # rbenv init
export PGDATA=/usr/local/var/postgres
export PGHOST=localhost
export GOPATH=$HOME/Documents/projects/fsproru/go
export PATH=/usr/local/bin:/usr/local/sbin:~/.cask/bin:~/.bin:$GOPATH/bin:$PATH

export ALTERNATE_EDITOR=emacs EDITOR=emacsclient VISUAL=emacsclient
export GIT_EDITOR=/usr/local/bin/emacsclient

export HISTCONTROL=ignoredups
shopt -s histappend

export LANG=en_US.UTF-8

# pass <C-s> and other terminal keys to vim
stty -ixon

if [ -f ~/.aliases ]; then
  . ~/.aliases
fi

GIT_DUET_GLOBAL=true

# Path to the bash it configuration
export BASH_IT="$HOME/.bash_it"
export BASH_IT_THEME='bobby'

# Load Bash It
source $BASH_IT/bash_it.sh
